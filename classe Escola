package escola;


public class Escola {
    
public static void main(String[] args) {
        Aluno a1 = new Aluno();
        a1.nome = "Joao";
        a1.materia = "Portugues";
        a1.matricula = 1234;
        a1.serie = 9;
        a1.aprovado = true;
        
        a1.status();
        a1.aprovado();
    }
    
}
